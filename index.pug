doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="css/style.css")
        title Title
    body 
        header.header 
            div.logo
                img(src="/images/logo.png", alt="Logo", class="logo")
            div.menu 
                each entrada in links
                    a(href=entrada.href)= entrada.text

        section.content 
            block content

        footer.footer
            p &copy;2023 Guilherme Drumond
                    